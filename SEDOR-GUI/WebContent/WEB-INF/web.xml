<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>SEDOR-GUI</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>index</servlet-name>
	</servlet>
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>diagnose</servlet-name>
		<servlet-class>br.com.sedor.control.ServletControl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>diagnose</servlet-name>
		<url-pattern>/diagnose</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>add_lesion</servlet-name>
		<servlet-class>br.com.sedor.control.ServletControlLesion</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>add_lesion</servlet-name>
		<url-pattern>/add_lesion</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>test</servlet-name>
		<servlet-class>br.com.sedor.control.ServletControlTest</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
</web-app>